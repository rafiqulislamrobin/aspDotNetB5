@model ConfirmFile
@{
    ViewData["Title"] = "ViewContacts";
}





<div class="row">
    <div class="col-md-12">
        <table class="table">
            <thead>
                <tr>
                    @{
                        foreach (var item in Model.headers)
                        {
                            <th>@item</th>
                        }
                    }
                </tr>
            </thead>
            <tbody>
               
                    @foreach (var item in Model.cont)
                    {
                        <tr>

                           @foreach (var v in item.Value)
                            {
                            <td>@v.Value</td>
                            }

                            
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
    <div class="col-md-6">
        <form asp-action="UploadHistory" asp-controller="DataImporter" method="post" enctype="multipart/form-data">
            <div class="form-group">

                <button type="submit" class="btn btn-success" style="color: white;background-color: #343a40;border-color:black">Upload</button>

            </div>
        </form>

    </div>
